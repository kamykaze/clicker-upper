{
    "manifest_version": 2,
    "name": "Clicker Upper",
    "version": "0.9.6",
    "description": "A set of enhancements for [ClickUp](https://app.clickup.com).",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery.min.js","content.js"],
        "matches": [
            "https://app.clickup.com/*"
        ]
    }],
    "options_page": "options.html",
    "permissions": [
        "https://app.clickup.com/",
        "storage"
    ]
}
